<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<Pane prefHeight="276.0" prefWidth="284.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <TextArea fx:id="gameLogDisplay" layoutX="21.0" layoutY="116.0" prefHeight="99.0" prefWidth="244.0" />
      <Label layoutX="21.0" layoutY="34.0" text="IP of Host:" />
      <Label layoutX="36.0" layoutY="82.0" text="Port:" />
      <Button layoutX="219.0" layoutY="30.0" mnemonicParsing="false" onAction="#hostIPCopyToClipboard" prefHeight="25.0" prefWidth="47.0" text="Copy " />
      <Text fx:id="hostIP" layoutX="85.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="123.0" />
      <Text fx:id="portNum" layoutX="76.0" layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="102.0" />
      <Button layoutX="185.0" layoutY="78.0" mnemonicParsing="false" onAction="#portNumCopyToClipboard" prefHeight="25.0" prefWidth="47.0" text="Copy " />
      <Button fx:id="startGameButton" layoutX="109.0" layoutY="231.0" mnemonicParsing="false" onAction="#startGame" text="Start Game" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</Pane>
