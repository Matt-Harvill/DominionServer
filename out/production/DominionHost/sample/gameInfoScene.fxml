<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>


<StackPane prefHeight="97.0" prefWidth="195.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GameInfoController">
   <children>
      <ChoiceBox fx:id="choiceBox" maxHeight="25.0" maxWidth="40.0" minHeight="25.0" minWidth="40.0" prefHeight="25.0" prefWidth="40.0" StackPane.alignment="TOP_RIGHT" />
      <Label text="Number of Players:" StackPane.alignment="TOP_LEFT" />
      <Button mnemonicParsing="false" onAction="#submitNumPlayers" text="Submit" StackPane.alignment="BOTTOM_CENTER" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</StackPane>
